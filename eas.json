{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_KEY": "API_KEY",
        "DEV_API_URL": "DEV_API_URL",
        "PROD_API_URL": "PROD_API_URL",
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
